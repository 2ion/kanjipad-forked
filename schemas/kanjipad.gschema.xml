<schemalist>
    <schema id="de.2ion.kanjipad" path="/de/2ion/kanjipad/" gettext-domain="kanjipad">
        <key name="kanjifont" type="s">
            <default>"Sans 18"</default>
        </key>
        <key name="delegatecmd" type="s">
            <default>"notify-send kanjipad %s"</default>
        </key>
        <key name="delegatefile" type="s">
            <default>"/dev/null"</default>
        </key>
    </schema>
</schemalist>
